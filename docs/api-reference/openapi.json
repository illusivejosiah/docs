    "/v1/proxies/auth/whitelist/{proxyName}": {
      "patch": {
        "description": "Sets the whitelisted IP addresses authorized for a proxy server.",
        "parameters": [
          {
            "name": "proxyName",
            "in": "path",
            "description": "Replace `{proxyName}` in the url path with `YOUR_PROXY`.",
            "required": true,
