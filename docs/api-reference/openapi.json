    "/v1/proxies/auth/basic/{proxyName}": {
      "patch": {
        "description": "Updates the basic authentication details of a proxy server.",
        "parameters": [
          {
            "name": "proxyName",
            "in": "path",
            "description": "Replace `{proxyName}` in the url path with `YOUR_PROXY`.",
