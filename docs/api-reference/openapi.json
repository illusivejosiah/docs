            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "enable": {
                    "type": "boolean",
                    "description": "IPv4 status value. If `enable` = `true`, then the proxy will have IPv4 enabled. Default: `true`",
                    "example": "true"
                  }
                },
                "required": [
                  "enable"
                ]
              }
            }
          }
        },
        "security": [
          {
            "bearerAuth": []
