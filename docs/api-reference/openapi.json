            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "enable": {
                    "type": "boolean",
                    "description": "Auto change external IP status value. If `enable` = `true`, then the proxy will have auto change external IP enabled. Default: `false`",
                    "example": true
                  },
                  "rate": {
                    "type": "integer",
                    "description": "Time interval for automatic IP change, in minutes. Default: `10`",
                    "example": 10
                  }
                },
                "required": [
                  "enable"
                ]
              }
            }
          }
        },
        "security": [
          {
            "bearerAuth": []
