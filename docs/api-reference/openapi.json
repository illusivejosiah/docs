        "requestBody": {
          "description": "Basic authentication details",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "username": {
                    "type": "string",
                    "description": "Username for authentication e.g., `john`. If `null`, a random username will be generated.",
                    "example": "john"
                  },
                  "password": {
                    "type": "string",
                    "description": "Password for authentication e.g., `wick05`. If `null`, a random password will be generated.",
                    "example": "wick05"
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Server response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "ok": {
                      "type": "boolean",
                      "description": "Indicates whether the request was successful e.g., `true`"
                    },
                    "message": {
                      "type": "string",
                      "description": "Success message",
                      "example": "Both username and password were updated."
                    },
                    "data": {
                      "$ref": "#/components/schemas/proxy-basic-auth"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Unexpected error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
