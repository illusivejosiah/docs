    "/v1/proxies/autochangeip/{proxyName}": {
      "post": {
        "description": "Sets auto change external IP settings of the proxy server.",
        "parameters": [
          {
            "name": "proxyName",
            "in": "path",
            "description": "Replace `{proxyName}` in the url path with `YOUR_PROXY`.",
